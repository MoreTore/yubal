# Yubal Configuration

# Server
YUBAL_PORT=8000               # Internal port (default: 8000)

# Audio
YUBAL_AUDIO_FORMAT=opus       # opus, mp3, m4a (default: opus)
YUBAL_AUDIO_QUALITY=0         # 0 (best) to 10 (worst) (default: 0)

# Timezone
YUBAL_TZ=Europe/Madrid        # IANA format (default: UTC)
